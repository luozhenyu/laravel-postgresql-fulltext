{
    "name": "luozhenyu/laravel-postgresql-fulltext",
    "description": "Add fulltext and inherited table support to postgresql",
    "license": "MIT",
    "authors": [
        {
            "name": "luozhenyu",
            "email": "lzy@luozy.cn"
        }
    ],
    "keywords": [
        "laravel",
        "postgresql",
        "database",
        "fulltext",
        "inherit"
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.5",
        "illuminate/database": "^5.5",
        "ext-pgsql": "*"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "LuoZhenyu\\PostgresFullText\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LuoZhenyu\\PostgresFullText\\PostgresqlSchemaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
