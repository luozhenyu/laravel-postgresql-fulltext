{
  "name": "luozhenyu/laravel-postgresql-fulltext",
  "description": "Add fulltext and inherited table support to postgresql",
  "license": "MIT",
  "authors": [
    {
      "name": "luozhenyu",
      "email": "lzy@luozy.cn"
    }
  ],
  "keywords": [
    "laravel",
    "postgresql",
    "database",
    "fulltext",
    "inherit"
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/support": "~5.4",
    "illuminate/database": "~5.4"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "LuoZhenyu\\PostgresFullText\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LuoZhenyu\\PostgresFullText\\PostgresqlSchemaServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
